.hl_header .container-fluid
/* Custom Header */ {
    background: var(--top-navbar-style-gradient-header-1);
    background: -moz-linear-gradient(
            -45deg,
            var(--top-navbar-style-gradient-header-1) 0%,
            var(--top-navbar-style-gradient-header-2) 59.5%,
            var(--top-navbar-style-gradient-header-line-1) 59.5%,
            var(--top-navbar-style-gradient-header-line-2) 60%,
            var(--top-navbar-style-gradient-header-3) 60%,
            var(--top-navbar-style-gradient-header-4) 100%
