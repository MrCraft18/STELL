.sidebar-v2-location #sidebar-v2 .hl_collapse-button:hover {
    -webkit-animation: pulse-collapse 2s infinite;
    animation: pulse-collapse 2s infinite;
}

@keyframes bounceleft {
    from {
        -webkit-transform: translateX(0);
    }

    to {
        -webkit-transform: translateX(-2px);
    }
}

@keyframes pulse-collapse {
    0% {
        transform: scale(0.95);
